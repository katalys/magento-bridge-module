{
    "name": "katalys/magento-bridge-module",
    "description": "Magento module for Katalys Integration",
    "type": "magento2-module",
    "version": "1.1.2",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "katalys/php-utils": "v1.1.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Katalys\\Shop\\": ""
        }
    },
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/katalys/composer-1o.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/katalys/paseto-php.git"
        }
    ]
}
